<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DontExpireFinal.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DontExpireFinal"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title="Don't Expire"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent Title="Preferences" ContentTemplate="{DataTemplate local:Prefs}"
        Route="Prefs" />
    <ShellContent Title="Add" ContentTemplate="{DataTemplate local:Add}"
        Route="Add" />

</Shell>

